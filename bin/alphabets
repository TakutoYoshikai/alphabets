#!/bin/bash

if [ "$1" = "-a" ]; then
  echo abcdefghijklmnopqrstuvwxyz
fi
if [ "$1" = "-A" ]; then
  echo ABCDEFGHIJKLMNOPQRSTUVWXYZ
fi
if [ "$1" = "-n" ]; then
  echo 0123456789
fi

if [ "$1" = "-all" ]; then
  echo `alphabets -a``alphabets -A``alphabets -n`
fi

if [ "$1" = "" ]; then
  alphabets -a
fi
